# Auto detect text files and perform LF normalization
* text=auto

# SQL files
*.sql text eol=lf

# Scripts
*.sh text eol=lf
*.ps1 text eol=crlf
*.cmd text eol=crlf
*.bat text eol=crlf

# Config files
*.yml text eol=lf
*.yaml text eol=lf
*.json text eol=lf
*.conf text eol=lf

# Documentation
*.md text eol=lf
*.txt text eol=lf

# Source code
*.cs text eol=lf
*.js text eol=lf
*.ts text eol=lf

# Binary files
*.dll binary
*.exe binary
*.zip binary
*.tar.gz binary
